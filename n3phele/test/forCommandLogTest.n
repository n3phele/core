name	   :  forCommandLog
description: a simple for loop
version	   : 1.0
preferred  : true
public	   : true
icon	   : http://www.n3phele.com/icons/custom
parameters :						  
	int n=3					# number of iterations
	int concurrent=2		# number of concurrent operations
EC2: # Amazon EC2
	FOR $$i : $$n : $$concurrent
		LOG log $$name $$i
		LOG log2 $$name $$i
